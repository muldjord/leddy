<?xml version="1.0" encoding="UTF-8"?>
<theme name="Default">
  <rotation>
    <rss url="http://rss.slashdot.org/Slashdot/slashdotMain"
	 background="random"
	 font="small"
	 fontcolor="complimentary"
	 showsource="true"
	 waveheight="4"
	 wavelength="60"/>
    <transition name="random"/>
    <weather duration="10000"
	     city="Copenhagen"
	     key="fe9fe6cf47c03d2640d5063fbfa053a2"
	     fontcolor="complimentary"
	     cityfont="tiny"
	     cityx="0"
	     cityy="9"
	     cityspacing="0,1,1,0"
	     tempfont="tiny"
	     tempx="0"
	     tempy="2"
	     tempspacing="0,1,0"/>
    <transition name="random"/>
    <timedate duration="10000"
	      background="random"
	      fontcolor="complimentary"
	      timefont="tiny"
	      timeformat="HH:mm"
	      timex="0"
	      timey="9"
	      timespacing="0,1,1,0"
	      datefont="tiny"
	      dateformat="ddMMM"
	      datex="0"
	      datey="2"
	      datespacing="0,1,0"/>
    <transition name="random"/>
    <animation name="random"/>
    <transition name="random"/>
  </rotation>
</theme>
